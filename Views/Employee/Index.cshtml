@model List<FirstMVCapp.Models.Domain.Employee>
@{
}

<h1>Employee</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Salary</th>
            <th>DOB</th>
            <th>Department</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var empoyee in Model)
        {
            <tr>
                <td>@empoyee.Id</td>
                <td>@empoyee.Name</td>
                <td>@empoyee.Email</td>
                <td>@empoyee.Salary</td>
                <td>@empoyee.DOB.ToString("dd-MM-yyyy")</td>
                <td>@empoyee.Department</td>
                <td><a href="Employee/View/@empoyee.Id">View</a></td>
            </tr>
        }
    </tbody>
</table>
